      "name": "GPL-3.0"
    },
    "title": "provisioning-api",
    "version": "1.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/pubkeys": {
      "get": {
        "description": "Returns list of resources.",
        "operationId": "getPubkeyList",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/v1.Pubkey"
                  },
                  "type": "array"
                }
              }
            },
            "description": "Returned on success."
          },
          "500": {
            "$ref": "#/components/responses/InternalError"
          }
        }
      }
    },
    "/pubkeys/{ID}": {
      "get": {
        "description": "Returns a particular resource.",
        "operationId": "getPubkeyByID",
        "parameters": [
          {
            "description": "Database ID of resource.",
            "in": "path",
            "name": "ID",
            "required": true,
            "schema": {
              "format": "int64",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/v1.Pubkey"
                }
              }
            },
            "description": "Returned on success."
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalError"
          }
        }
      }
    },
    "/sources": {
      "get": {
        "description": "Return list of provisioning sources",
        "operationId": "getListSources",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/v1.Source"
                  },
                  "type": "array"
                }
              }
            },
            "description": "Returned on success."
          },
          "500": {
            "$ref": "#/components/responses/InternalError"
          }
        }
      }
    },
    "/sources/{ID}": {
      "get": {
        "description": "Return a provisioning source by id",
        "operationId": "getSourceID",
        "parameters": [
          {
            "description": "Database ID of resource.",
            "in": "path",
            "name": "ID",
            "required": true,
            "schema": {
              "format": "int64",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/v1.Source"
                }
              }
            },
            "description": "Return on success."
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalError"
          }
        }
      }
    }
  },
  "servers": [
    {
      "description": "Local development",
      "url": "http://0.0.0.0:{port}/api/{applicationName}",
      "variables": {
        "applicationName": {
          "default": "provisioning"
        },
        "port": {
          "default": "8000"
        }
      }
    }
  ]
}